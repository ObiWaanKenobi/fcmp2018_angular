{
  "extends": "airbnb",
  "env": {
    "node":true,
    "mocha":true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "object-curly-spacing": 0,
    "consistent-return": 0,
    "no-console": 0,
    "prefer-destructuring": 0,
    "no-unused-vars": 0,
    "class-methods-use-this": 0,
    "max-len": ["error", {"code": 120, "ignoreComments": true, "ignoreUrls": true, "ignoreStrings": true}]
  }
}
